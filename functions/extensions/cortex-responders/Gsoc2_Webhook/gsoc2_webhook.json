{
  "name": "Gsoc2_webhook",
  "version": "1.0",
  "author": "@azgaviperr",
  "url": "https://github.com/gsoc2/gsoc2",
  "license": "AGPL-V3",
  "description": "Execute a webhook in Gsoc2",
  "dataTypeList": ["thehive:case", "thehive:alert", "thehive:case_artifact"],
  "command": "Gsoc2_Webhook/gsoc2_webhook.py",
  "baseConfig": "Gsoc2_Webhook",
  "configurationItems": [
    {
      "name": "webhook_url",
      "description": "The URL to your gsoc2 instance",
      "type": "string",
      "multi": false,
      "required": true
    },
		{
      "name": "verifyssl",
      "description": "Verify SSL certificate",
      "type": "boolean",
      "multi": false,
      "required": true,
      "defaultValue": true
    }
  ]
}
